{
  "name": "aware-mvp",
  "version": "1.0.16",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clear-cache": "expo start --clear",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:services": "jest src/services",
    "test:models": "jest src/models",
    "test:viewModels": "jest src/viewModels",
    "test:screens": "jest app/screens",
    "test:e2e": "playwright test",
    "test:e2e:open": "playwright test && npx playwright show-report",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --headed --debug",
    "test:e2e:slow": "playwright test --headed --slow-mo=500",
    "test:e2e:nav": "playwright test -c src/__tests__/e2e/navigation.config.ts",
    "test:e2e:nav:headed": "playwright test -c src/__tests__/e2e/navigation.config.ts --headed",
    "test:e2e:nav:console": "npx playwright test -c src/__tests__/e2e/navigation.config.ts --headed --reporter=line",
    "test:e2e:nav:debug": "playwright test -c src/__tests__/e2e/navigation.config.ts --headed --debug",
    "test:e2e:nav:verbose": "DEBUG=pw:api npx playwright test -c src/__tests__/e2e/navigation.config.ts --reporter=dot",
    "test:e2e:nav:simple": "npx playwright test -c src/__tests__/e2e/navigation.config.ts --grep \"should navigate from intro through sign-in to sign-up\" --reporter=line",
    "test:report": "npx playwright show-report",
    "test:llm": "cd browser-use-tests && python run_tests.py",
    "test:llm:headed": "cd browser-use-tests && python run_tests.py --verbose",
    "test:llm:signup": "cd browser-use-tests && python run_tests.py --test tests/test_signup_mbti_flow.py --verbose",
    "test:llm:insights": "cd browser-use-tests && python run_tests.py --test tests/test_user_insights.py --verbose",
    "test:llm:intro": "cd browser-use-tests && python run_tests.py --test tests/test_intro_screen.py --verbose",
    "test:llm:intro:debug": "cd browser-use-tests && python run_tests.py --test tests/test_intro_screen.py --verbose --debug --timeout 90000",
    "test:llm:mbti": "cd browser-use-tests && python run_tests.py --test tests/test_mbti_assessment.py --verbose",
    "test:llm:mbti:debug": "cd browser-use-tests && python run_tests.py --test tests/test_mbti_assessment.py --verbose --debug --timeout 90000",
    "test:llm:setup": "cd browser-use-tests && pip install -r requirements.txt",
    "generate:types": "supabase gen types --lang=typescript --local > src/models/database.types.ts",
    "generate:types:remote": "supabase gen types --lang=typescript --project-id mnrkzzcanaznlpogpxzf > src/models/database.types.ts",
    "ios:clean": "rm -rf ios/build && rm -rf ios/Pods && rm -rf ios/DerivedData && rm -rf ios/Pods.xcodeproj && rm -rf ios/Pods.xcworkspace",
    "ios:prebuild:clean": "expo prebuild --platform ios --clean",
    "ios:prebuild": "expo prebuild --platform ios",
    "ios:run:release": "expo run:ios --configuration Release",
    "ios:run:debug": "expo run:ios --configuration Debug",
    "ios:run:device": "expo run:ios --device",
    "ios:run:sim:only:debug": "expo run:ios --configuration Debug --device-name \"iPhone 15\" --no-bundler",
    "ios:run:sim:only:release": "expo run:ios --configuration Release --no-dev --simulator",
    "lint": "echo 'not implemented yet'",
    "db:reset": "supabase db reset --local",
    "db:reset:remote": "echo 'y' | supabase db reset --linked"
  },
  "dependencies": {
    "@builder.io/dev-tools": "^1.3.3",
    "@builder.io/react": "^8.0.0",
    "@expo/metro-runtime": "~4.0.0",
    "@legendapp/state": "^3.0.0-beta.19",
    "@mistralai/mistralai": "^1.3.5",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-picker/picker": "^2.11.0",
    "@supabase/supabase-js": "^2.45.2",
    "dayjs": "^1.11.13",
    "expo": "~52.0.17",
    "expo-crypto": "^14.0.1",
    "expo-document-picker": "^13.0.1",
    "expo-linking": "^7.0.3",
    "expo-router": "^4.0.11",
    "expo-status-bar": "~2.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "mustache": "^4.2.0",
    "nanoid": "3.3.8",
    "neverthrow": "^6.1.0",
    "openai": "^4.77.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "2.21.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.6.0",
    "react-native-svg": "15.2.0",
    "react-native-toast-message": "^2.2.1",
    "react-native-ui-datepicker": "^2.0.4",
    "react-native-ui-lib": "^7.34.6",
    "react-native-web": "~0.19.13",
    "reflect-metadata": "^0.1.13",
    "supawright": "^0.3.0",
    "tsyringe": "^4.8.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-decorators": "^7.29.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/mustache": "^4.2.3",
    "@types/node": "^20.17.10",
    "@types/react": "~18.3.14",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "react-test-renderer": "^18.3.1",
    "supabase": "^1.145.4",
    "typescript": "^5.7.2"
  },
  "private": true,
  "overrides": {
    "react-server-dom-webpack": "19.0.2"
  }
}
